{
  "name": "web",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:dash": "turbo run dev --filter=dashboard",
    "build:dash": "turbo run build --filter=dashboard",
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^2.8.2",
    "turbo": "^1.7.0"
  },
  "engines": {
    "node": "18.12.1"
  },
  "packageManager": "pnpm@7.24.3"
}
